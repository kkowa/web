{
  "name": "kkowa/apps/web",
  "dockerComposeFile": ["../docker-compose.yml", "./override.yml"],
  "runServices": ["web"],
  "service": "web",
  "overrideCommand": true,
  "initializeCommand": "sh ./.devcontainer/initializeCommand.sh",
  "onCreateCommand": "sh ./.devcontainer/onCreateCommand.sh",
  "updateContentCommand": "sh ./.devcontainer/updateContentCommand.sh",
  "postCreateCommand": "sh ./.devcontainer/postCreateCommand.sh",
  "postStartCommand": "sh ./.devcontainer/postStartCommand.sh",
  "postAttachCommand": "sh ./.devcontainer/postAttachCommand.sh",
  "shutdownAction": "stopCompose",
  "workspaceFolder": "/var/app",
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "mhutchie.git-graph",
    "eamodio.gitlens",
    "ldez.ignore-files",
    "gruntfuggly.todo-tree",
    "redhat.vscode-yaml",
    "streetsidesoftware.code-spell-checker",
    "tamasfe.even-better-toml",
    "GitLab.gitlab-workflow",
    "EditorConfig.EditorConfig"
  ]
}
